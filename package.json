{
  "name": "@switchboard-xyz/switchboard-v2",
  "description": "switchboard v2 repo containing type definitions, libraries, and examples",
  "private": true,
  "workspaces": [
    "tasks/v2-task-library",
    "tools/v2-utils-ts",
    "libraries/ts",
    "tools/v2-cli",
    "tasks/v2-task-runner",
    "examples/feed-parser",
    "examples/feed-walkthrough",
    "feeds/feed-observer",
    "programs/anchor-feed-parser",
    "programs/spl-feed-parser",
    "docs"
  ],
  "scripts": {
    "start": "echo \"Error: no start script specified\" && exit 1",
    "purge:deps": "find . -name \"node_modules\" -exec rm -rf {} \\;",
    "simulate:jobs:all": "ts-node scripts/simulate run --dir job-directory",
    "simulate:jobs:exchanges": "ts-node scripts/simulate run --dir job-directory/exchanges",
    "simulate:jobs:lending": "ts-node scripts/simulate run --dir job-directory/lendingRate"
  },
  "peerDependencies": {
    "@project-serum/anchor": "0.22.1",
    "@solana/web3.js": "1.37.1",
    "mocha": "9.2.2"
  },
  "dependencies": {
    "@solana/web3.js": "1.37.1",
    "yargs": "^17.4.0"
  },
  "devDependencies": {
    "@types/big.js": "^6.1.3",
    "@types/eslint": "^8.4.1",
    "@types/long": "^4.0.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@types/prettier": "^2.4.4",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "estrella": "^1.4.1",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "shx": "^0.3.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  }
