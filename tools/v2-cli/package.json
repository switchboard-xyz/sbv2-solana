{
  "name": "@switchboard-xyz/switchboardv2-cli",
  "description": "command line tool to interact with switchboard v2",
  "version": "0.1.6",
  "license": "MIT",
  "repository": "switchboard-xyz/switchboardv2-cli",
  "bugs": "https://github.com/switchboard-xyz/switchboardv2-cli/issues",
  "homepage": "https://github.com/switchboard-xyz/switchboardv2-cli",
  "bin": {
    "sbv2": "./bin/run"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sbv2",
    "dirname": "@switchboard-xyz/sbv2-cli",
    "plugins": [
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/config"
    ],
    "topics": {
      "aggregator": {
        "description": "interact with a switchboard aggregator account"
      },
      "crank": {
        "description": "interact with a switchboard crank account"
      },
      "queue": {
        "description": "interact with a switchboard oracle queue account"
      },
      "job": {
        "description": "interact with a switchboard job account"
      },
      "oracle": {
        "description": "interact with a switchboard oracle account"
      },
      "print": {
        "description": "find and print a switchboard account by public key for a given cluster"
      },
      "json": {
        "description": "create and manage an oracle queue from a json file"
      }
    }
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "doc": "oclif-dev readme",
    "doc:out": "oclif-dev readme --multi --dir ../docs-v2/docs/developers/cli && cd ../docs-v2/docs/developers/cli/ && for f in *.md; do mv \"${f%.md}.md\" \"_${f%.md}.md\"; sed -i \"\" '1,2d' \"_${f%.md}.md\"; done",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "fmt": "prettier --write 'src/**/*.ts'",
    "lint": "eslint . --ext .ts --config .eslintrc.json --fix"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^3.10.1",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.2",
    "@oclif/parser": "^3.8.6",
    "@oclif/plugin-update": "^1.5.0",
    "@oclif/plugin-warn-if-update-available": "^1.7.3",
    "@project-serum/anchor": "^0.22.1",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.37.1",
    "@switchboard-xyz/switchboard-api": "^0.2.190",
    "@switchboard-xyz/switchboard-v2": "^0.0.72",
    "assert": "^2.0.0",
    "big.js": "^6.1.1",
    "chalk": "^4.1.2",
    "cli-ux": "^5.6.4",
    "decimal.js": "^10.3.1",
    "node-fetch": "^2.6.6",
    "readline-sync": "^1.4.10",
    "tslib": "^1.14.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.5",
    "@oclif/test": "^2.0.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.5.0",
    "prettier-plugin-organize-imports": "^2.3.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=8.5.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif",
    "switchboard",
    "solana",
    "oracle"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts"
}
